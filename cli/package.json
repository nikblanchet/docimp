{
  "name": "docimp-cli",
  "version": "1.1.0-beta.1",
  "description": "TypeScript CLI for DocImp - impact-driven documentation coverage tool",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "docimp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:integration": "node --test src/__tests__/integration/*.mjs",
    "test:all": "npm test && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "lint:jsdoc": "eslint src --ext .ts,.js,.mjs,.cjs",
    "format": "prettier --write --ignore-path ../.prettierignore \"src/**/*.{ts,js,mjs,cjs}\" \"../plugins/**/*.{js,mjs,cjs}\"",
    "format:check": "prettier --check --ignore-path ../.prettierignore \"src/**/*.{ts,js,mjs,cjs}\" \"../plugins/**/*.{js,mjs,cjs}\"",
    "format:ts": "prettier --write \"src/**/*.ts\"",
    "format:js": "prettier --write \"src/**/*.{js,mjs,cjs}\" \"../plugins/**/*.{js,mjs,cjs}\"",
    "prepare": "husky"
  },
  "keywords": [
    "documentation",
    "static-analysis",
    "typescript",
    "javascript",
    "python"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0 <25.0.0"
  },
  "dependencies": {
    "@types/prompts": "^2.4.9",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "comment-parser": "^1.4.1",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "tmp": "^0.2.3",
    "typescript": "^5.7.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5"
  },
  "lint-staged": {
    "*.{ts,js,mjs,cjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.py": [
      "ruff format",
      "ruff check --fix"
    ]
  }
}
